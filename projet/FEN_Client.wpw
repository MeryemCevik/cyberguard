   -
     name : EnregistrerUtilisateurClient
     procedure_id : 1553558661025328290
     type_code : 14
     code : |1+
      // Résumé : Enregistre une liaison entre un utilisateur et un client dans la table Utilisateurs_Clients.
      // Syntaxe : EnregistrerUtilisateurClient(clientID)
      //
      // Paramètres :
      //   clientID : Entier, l'ID du client à lier à l'utilisateur.
      //
      // Valeur de retour :
      //   Aucune
      //
      // Exemple :
      //   EnregistrerUtilisateurClient(1)
      
      procédure EnregistrerUtilisateurClient(id_client)
      	// Enregistrement dans la table Utilisateurs_Clients l'id client actuel et l'id utilisateur connecté
      	Utilisateur_Client.IDClient			= id_client
      	Utilisateur_Client.IDUtilisateur	= Id_u
      		
      	// Ajouter l'enregistrement
      	HAjoute(Utilisateur_Client)
      	
      	
      	
      	
      	
     type : 458752
