  procedures :
   -
     name : CreerCompte
     procedure_id : 1547532894939832876
     type_code : 14
     code : |1-
      // Résumé : Procédure permettant de créer un compte utilisateur dans la base de données avec des contrôles de saisie et de sécurité du mot de passe.
      // Syntaxe :
      //CreerCompte (<civilite>, <nom>, <prenom>, <email>, <mdp>, <verifmdp>)
      //
      // Paramètres :
      //	civilite : Chaîne - Indique la civilité de l'utilisateur (Mme, M, Autre).
      //	nom : Chaîne - Indique le nom de l'utilisateur.
      //	prenom : Chaîne - Indique le prénom de l'utilisateur.
      //	email : Chaîne - Indique l'adresse e-mail de l'utilisateur.
      //	mdp : Chaîne - Indique le mot de passe de l'utilisateur.
      //	verifmdp : Chaîne - Indique la confirmation du mot de passe de l'utilisateur.
      // Valeur de retour :
      // 	Aucune
      //
      // Exemple :
      // CreerCompte("Mme", "Nom", "Alice", "alice@email.com", "Test12345678$", "Test12345678$")
      //
      PROCÉDURE CreerCompte(civilite, nom, prenom, email, mdp, verifmdp)
      
      //Vérifier si toutes les données sont correctes
      toutEstCorrecte est un booléen = Vrai
      // Gérer le sélecteur civilité (Mme, M, Autre) et enregistrement dans la BDD
      valeurCivilite est une chaîne = ""
      
      SI civilite=0 ALORS
      	valeurCivilite="Mme"
      SINON SI civilite=1 ALORS
      	valeurCivilite="M"
      SINON SI civilite=2 ALORS
      	valeurCivilite="Autre"
      SINON
      	ToastAffiche("Veuillez remplir les champs.")
      	toutEstCorrecte=Faux
      	RETOUR
      FIN
      
      //Contrôle des champs
      SI nom="" OU prenom="" OU email="" OU mdp="" OU verifmdp="" ALORS
      	ToastAffiche("Veuillez remplir les champs.")
      	toutEstCorrecte=Faux
      	RETOUR
      FIN
      
      //Vérif email
      SI EmailVérifieAdresse(email,emailValideSyntaxe)<>emailAdresseValide _ET_ PAS VérifieExpressionRégulière(SAI_Email, "^(0|\+33)[1-9]([-. ]?[0-9]{2}){4}") ALORS
      	ToastAffiche("L'identification saisie ne correspond pas a un E-mail.")
      	DonneFocus(email)
      	toutEstCorrecte=Faux
      	RETOUR
      FIN
      
      //Enregistrement dans la BDD
      //Vérification du mot de passe
      SI VerificationMotDePasse(mdp,verifmdp) = Vrai ALORS
      	Si toutEstCorrecte=Vrai ALORS
      		// Remplissage des champs d'édition
      		Utilisateur.Civilite	= valeurCivilite
      		Utilisateur.Nom_utilisateur		= nom
      		Utilisateur.Prenom_utilisateur		= prenom
      		Utilisateur.Email		= email
      		Utilisateur.MotDePasse	= mdp
      		Utilisateur.IDRole = 1
      		
      		//Enregistrement
      		Utilisateur.MotDePasse("BTSSIO1234")
      		SI HErreurMotDePasse() = Vrai ALORS
      			Erreur("Mot de passe invalide, enregistrement échoué.")
      		SINON
      			HAjoute(Utilisateur)
      			ToastAffiche("Utilisateur enregistré avec succès.")
      			Remettre_fen_a_zero(civilite, nom, prenom, email, mdp, verifmdp)
      		FIN
      	FIN
      FIN
     type : 458752
   -
     name : VerificationMotDePasse
     procedure_id : 1547567954744602790
     type_code : 14
     code : |1-
      // Résumé : Procédure de vérification de la conformité d'un mot de passe avec des critères de sécurité.
      // Syntaxe :
      //[ <Résultat> = ] VerificationMotDePasse (<mdp>, <verifmdp>)
      //
      // Paramètres :
      //	mdp : Chaîne - Mot de passe à vérifier.
      //	verifmdp : Chaîne - Confirmation du mot de passe.
      // Valeur de retour :
      // 	booléen : <indiquez ici le rôle de la valeur de retour>
      //
      // Exemple :
      // VerificationMotDePasse("MotDePasse123", "MotDePasse123")
      //
      PROCÉDURE VerificationMotDePasse(mdp,verifmdp)
      //1 - Crypter le mdp
      //mdpCrypt est une chaine = Crypte(mdp,"r_user")
      
      //Déclaration et initialisation
      mdpValide	est un booléen	= Vrai
      //Police1		est une Police
      longueurMdp	est un entier	= 12
      //Police1.Barré			= Faux
      
      //2 - Vérifier si mdp = verifmdp
      SI mdp <> verifmdp ALORS
      	ToastAffiche("Les mots de passe sont différents")
      	mdpValide = Faux
      	DonneFocus(mdp)
      	RETOUR mdpValide
      FIN
      //3 - Vérifier si mdp est conforme au RGPD
      //Ne contient pas 12 caractères ou plus
      SI Taille(mdp) < longueurMdp ALORS
      	//Police1.Barré			= Vrai
      	//LIB_caractere.Police	= Police1
      	LIB_caractere.Couleur = RougeClair
      	mdpValide				= Faux
      	DonneFocus(mdp)
      SINON
      	LIB_caractere.Couleur = Noir
      FIN 
      //Ne contient pas au moins une lettre majuscule
      SI VérifieExpressionRégulière(mdp, ".*[A-Z].*") = Faux ALORS
      	//Police1.Barré			= Vrai
      	//LIB_maj.Police			= Police1
      	LIB_maj.Couleur = RougeClair
      	mdpValide				= Faux
      	DonneFocus(mdp)
      SINON
      	LIB_maj.Couleur = Noir
      FIN 
      //Ne contient pas au moins une lettre minuscule
      SI VérifieExpressionRégulière(mdp, ".*[a-z].*") = Faux ALORS
      	//Police1.Barré			= Vrai
      	//LIB_min.Police			= Police1
      	LIB_min.Couleur = RougeClair
      	mdpValide				= Faux
      	DonneFocus(mdp)
      SINON
      	LIB_min.Couleur = Noir
      FIN
      //Ne contient pas au moins un chiffre
      SI VérifieExpressionRégulière(mdp, ".*[0-9].*") = Faux ALORS
      	//Police1.Barré			= Vrai
      	//LIB_chiffre.Police		= Police1
      	LIB_chiffre.Couleur = RougeClair
      	mdpValide				= Faux
      	DonneFocus(mdp)
      SINON
      	LIB_chiffre.Couleur = Noir
      FIN
      
      //Ne contient pas au moins un caractère spécial
      SI VérifieExpressionRégulière(mdp,".*[+*/%*_{}()@#|&\=$!²³éèêëíìîïàáâäóòôöúùûüýÿç.;:?°§,~£µ<>].*")=Faux ALORS
      	//Police1.Barré							= Vrai
      	//LIB_caractere_special.Police			= Police1
      	LIB_caractere_special.Couleur = RougeClair
      	mdpValide								= Faux
      	DonneFocus(mdp)
      SINON 
      	LIB_caractere_special.Couleur = Noir
      FIN
      
      RENVOYER mdpValide
