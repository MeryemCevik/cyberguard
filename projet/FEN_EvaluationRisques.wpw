     name : InitialisationCOMBO_client
     procedure_id : 1562239356869615173
     type_code : 14
     code : |1-
      // Résumé : <indiquez ici ce que fait la procédure>
      // Syntaxe :
      // InitialisationCOMBO_client ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      // Exemple :
      // <Indiquez ici un exemple d'utilisation>
      //
      procédure InitialisationCOMBO_client()
      	//np stocke la concét° de prénom et nom client
      	np est une chaîne
      	//Recherche les clients d'un utilisateur donné pour les affichés
      	POUR TOUT Utilisateur_Client
      		SI Utilisateur_Client.IDUtilisateur = Id_u ALORS
      			POUR TOUT Client
      				SI Client.IDClient = Utilisateur_Client.IDClient ALORS
      					np = Client.Nom_client + " " + Client.Prenom_client
      					ListeAjoute(COMBO_Client, np)
      				FIN
      			FIN
      		FIN
      	FIN
     type : 458752
   -
     name : TrouverIdClient
     procedure_id : 1562241147878472931
     type_code : 14
     code : |1+
      // Résumé : <indiquez ici ce que fait la procédure>
      // Syntaxe :
      // TrouverIdClient ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      // Exemple :
      // <Indiquez ici un exemple d'utilisation>
      //
      procédure TrouverIdClient(nomClient)
      
      	valeurComboClient est une chaîne
      	valeurComboClient = nomClient
      	
      	// Extraction du Nom et Prénom à partir de la valeur de la combo
      	Nomm est une chaîne
      	Nomm = ExtraitChaîne(valeurComboClient, 1, " ")
      	Prenom est une chaîne
      	Prenom = ExtraitChaîne(valeurComboClient, 2, " ")
      	
      	//Recherche de l'id correspondant
      	ID_ClientSelectionne est un entier sur 8 octet
      	ID_ClientSelectionne = -1
      	
      	POUR TOUT Client
      		SI Client.Nom_client = Nomm ET Client.Prenom_client = Prenom ALORS
      			// Trouvé le client correspondant
      			ID_ClientSelectionne = Client.IDClient
      			//Info(ID_ClientSelectionne)
      			SORTIR // Sortir de la boucle puisque l'id a été trouvé
      		FIN
      	FIN
      	
      	RENVOYER ID_ClientSelectionne
     type : 458752
   -
     name : CreerEstimationsRisquesAuto
     procedure_id : 1562255239672532346
     type_code : 14
     code : |1-
      // Résumé : <indiquez ici ce que fait la procédure>
      // Syntaxe :
      //CreerEstimationsRisquesAuto ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      // Exemple :
      // <Indiquez ici un exemple d'utilisation>
      //
      procédure CreerEstimationsRisquesAuto()
      	//Hsupprime
      //	Hsupprimetout(Evaluation_Risque)
      	
      	//déclarations
      	NiveauRisque est un entier=0
      	type_eval est un chaine = "Automatique"
      	idClient est un entier sur 8 octets = TrouverIdClient(SAI_client)
      	ToutestBon est un booléen = Vrai
      	//MesureDeSecurite
      	PoidsMesureSecurite	est réel	= 0.33
      	//Nb Mesure de sécurité pour cet actif
      	NbmesuresSecurite	est un entier
      	
      	//Vérification client
      	si SAI_client="" alors
      		ToutestBon=Faux
      		ToastAffiche("Veuillez sélectionner un client")
      	FIN
      	
      	//Vérification si le client a au moins un actif
      	nbActif est un entier = 0
      	pour TOUT Actifs 
      		si Actifs.IDClient = idClient alors
      			nbActif=nbActif+1
      		FIN
      	FIN
      	
      	SI nbActif=0 ALORS
      		ToutestBon=Faux
      		ToastAffiche("Ce client ne contient pas d'actif. Veuillez créer au moins un actif pour ce client.",toastLong)
      	FIN
      	
      	//Création estimation de risque
      	//Calculs
      	si ToutestBon=Vrai ALORS
      		POUR TOUT Actifs 
      			//pour tout les actifs de ce client
      			si idClient = Actifs.IDClient ALORS
      				// Obtention de l'ID de l'actif actuel
      				ActifID est un entier sur 8 octets = Actifs.IDActifs
      				MesureDeSecuriteID est un entier sur 8 octet = Actifs.IDMesureDeSecurite
      				
      				//Calcul nb mesure de sécurité pour cet actif
      				NbmesuresSecurite	= CalculNbMesureDeSecurite(MesureDeSecuriteID)
      				//info(NbmesuresSecurite)
      				
      				POUR TOUT Actifs_Menace 
      					si Actifs_Menace.IDActifs=ActifID ALORS
      						MenaceID est un entier sur 8 octets = Actifs_Menace.idMenace
      						ImpactMenace est un entier = 0
      						//Recherche impact menace
      						pour TOUT Menace 
      							SI Menace.IDMenace = MenaceID ALORS
      								ImpactMenace= Menace.Niveau_risque
      							FIN
      						fin
      						//Recherche impact vulns
      						ImpactVulns est un entier = RechercheMoyenneImpactVulnerabilites(MenaceID)
      						//info(ImpactVulns)
      						//Calculs impact risque
      						//NiveauRisque est un entier = (ImpactVulns * ImpactMenace) - (NbmesuresSecurite * PoidsMesureSecurite)			
      						NiveauRisque = (ImpactVulns * ImpactMenace)
      						
      						// Insertion dans la table Evaluation_Risques
      						Evaluation_Risque.IDActifs = ActifID
      						Evaluation_Risque.IDMenace = MenaceID
      						Evaluation_Risque.Type_evaluation=type_eval
      						Evaluation_risque.Probabilite=ImpactVulns
      						Evaluation_risque.Gravite=ImpactMenace
      						Evaluation_Risque.Niveau_risque=NiveauRisque
      						Evaluation_Risque.ControleDeSecurite=NbmesuresSecurite
      						Evaluation_Risque.IDClient=idClient
      						
      						// Enregistrement dans la BDD
      						HAjoute(Evaluation_Risque)
      						
      						// Vérification de l'ajout
      						SI ErreurInfo() = "" ALORS
      //							ToastAffiche("Évaluation de risques enregistré avec succès.", toastLong)
      				
      						SINON
      							// En cas d'erreur lors de l'enregistrement, annuler l'ajout
      							//HSupprime(Utilisateur)
      							ToastAffiche("Une erreur est survenue lors de l'enregistrement.")
      						FIN
      					FIN
      				FIN
      				
      			FIN
      		FIN
      	FIN
      	ToastAffiche("Évaluations de risques enregistré avec succès.", toastLong)
      	
      
      	
     type : 458752
   -
     name : RechercheMoyenneImpactVulnerabilites
     procedure_id : 1563368052800155990
     type_code : 14
     code : |1+
      // Résumé : <indiquez ici ce que fait la procédure>
      // Syntaxe :
      //[ <Résultat> = ] RechercheMoyenneImpactVulnerabilites (<idMenace>)
      //
      // Paramètres :
      //	idMenace : <indiquez ici le rôle de idMenace>
      // Valeur de retour :
      // 	entier : <indiquez ici le rôle de la valeur de retour>
      //
      // Exemple :
      // <Indiquez ici un exemple d'utilisation>
      //
      // 	idMesureDeSecurite : <indiquez ici le rôle de idMesureDeSecurite>
      procédure RechercheMoyenneImpactVulnerabilites(idMenace)
      	// Initialisation de la somme des impacts et du nombre de vulnérabilités
      	// Déclaration des variables
      	NbVuln			est un entier	= 0
      	SommeImpacts	est un entier	= 0
      	MoyenneImpactVuln	est un réel = 0
      	ImpactVuln			est un entier = 0
      	
      	// Recherche des vulnérabilités pour la menace donnée
      	POUR TOUT Menace_Vulnerabilite
      		SI Menace_Vulnerabilite.IDMenace = idMenace ALORS
      			// Obtention de l'ID de la vulnérabilité
      			VulnID est un entier sur 8 octets = Menace_Vulnerabilite.IDVulnerabilite
      			
      			pour TOUT Vulnerabilite 
      				// Recherche de l'impact dans la table Vulnérabilité
      				SI Vulnerabilite.IDVulnerabilite = VulnID ALORS
      					SommeImpacts	= SommeImpacts + Vulnerabilite.Niveau_risque
      					NbVuln			= NbVuln + 1
      					
      				FIN
      			fin
      		FIN
      	FIN
      	
      	si NbVuln > 0
      		// Calcul de la moyenne et arrondi à l'entier le plus proche
      	 	MoyenneImpactVuln	= SommeImpacts / NbVuln
      	 	// Utilisation de la fonction Arrondi pour arrondir à l'entier le plus proche
      	 	ImpactVuln = Arrondi(MoyenneImpactVuln)
      	fin 	
      	
      	RENVOYER ImpactVuln
      
      
      
     type : 458752
   -
     name : CalculNbMesureDeSecurite
     procedure_id : 1563436776583992849
     type_code : 14
     code : |1+
      // Résumé : Calcul le nb de mesures de sécurité mise en place et le renvoie
      // Syntaxe :
      //[ <Résultat> = ] CalculNbMesureDeSecurite (<MesureDeSecuriteID>)
      //
      // Paramètres :
      // 	MesureDeSecuriteID : <indiquez ici le rôle de MesureDeSecuriteID>
      // Valeur de retour :
      // 	entier : <indiquez ici le rôle de la valeur de retour>
      //
      // Exemple :
      // <Indiquez ici un exemple d'utilisation>
      //
      procédure CalculNbMesureDeSecurite(MesureDeSecuriteID)
      
      	NbMesuresSecurite est un entier=0
      	
      	pour TOUT MesureDeSecurite
      		si MesureDeSecurite.IDMesureDeSecurite = MesureDeSecuriteID
      			// Vérification des critères de mesure de sécurité
      			SI MesureDeSecurite.Maj = Vrai ALORS
      				NbMesuresSecurite = NbMesuresSecurite + 1
      			FIN
      			SI MesureDeSecurite.Antivirus = Vrai ALORS
      				NbMesuresSecurite = NbMesuresSecurite + 1
      			FIN
      			SI MesureDeSecurite.Secu_mdp = Vrai ALORS
      				NbMesuresSecurite = NbMesuresSecurite + 1
      			FIN
      			SI MesureDeSecurite.Secu_double_authentif = Vrai ALORS
      				NbMesuresSecurite = NbMesuresSecurite + 1
      			FIN
      			SI MesureDeSecurite.Chiffrement_Donnees_Perso = Vrai ALORS
      				NbMesuresSecurite = NbMesuresSecurite + 1
      			FIN
      			SI MesureDeSecurite.Politiq_acces = Vrai ALORS
      				NbMesuresSecurite = NbMesuresSecurite + 1
      			FIN
      		FIN
      	FIN
      	
      	renvoyer NbMesuresSecurite
     type : 458752
   -
     name : SupprimerEstmationsAuto
     procedure_id : 1564852715624308411
     type_code : 14
     code : |1+
      // Résumé : <indiquez ici ce que fait la procédure>
      // Syntaxe :
      // SupprimerEstmationsAuto ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      // Exemple :
      // <Indiquez ici un exemple d'utilisation>
      //
      procédure SupprimerEstmationsAuto()
      
      //Demander confirmation avant de supprimer
      SI OuiNon("Voulez-vous vraiment supprimer toutes les évaluation de risques créer automatiquement ?") = Oui ALORS
      	//Supprimer toutes les estimations de risques créer automatiquement afin de créer
      	IMG_chargement..Visible=Vrai
      	POUR TOUT Evaluation_Risque AVEC "Type_evaluation = 'Automatique'"
      		HSupprime(Evaluation_Risque,HNumEnr())
      	FIN
      	pour TOUT Actifs_Menace 
      		HSupprime(Actifs_Menace,HNumEnr())
      	FIN
      //	HRecherchePremier(Evaluation_Risque,Type_evaluation,"Automatique")
      //	TANTQUE PAS HNumEnr()
      //		HSupprime(Evaluation_Risque)
      //		HSuivant(Evaluation_Risque)
      //	FIN
      	IMG_chargement..Visible=Faux
      SINON
      	ToastAffiche("Suppression annulée.")
      FIN
