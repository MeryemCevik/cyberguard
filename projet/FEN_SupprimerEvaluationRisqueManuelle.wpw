   -
     name : InitialisationCOMBO_client
     procedure_id : 1566322298827443413
     type_code : 14
     code : |1+
      // Résumé : <indiquez ici ce que fait la procédure>
      // Syntaxe :
      // InitialisationCOMBO_client ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      // Exemple :
      // <Indiquez ici un exemple d'utilisation>
      //
      procédure InitialisationCOMBO_client()
      	//np stocke la concét° de prénom et nom client
      	np est une chaîne
      	//Recherche les clients d'un utilisateur donné pour les affichés
      	POUR TOUT Utilisateur_Client
      		SI Utilisateur_Client.IDUtilisateur = Id_u ALORS
      			POUR TOUT Client
      				SI Client.IDClient = Utilisateur_Client.IDClient ALORS
      					np = Client.Nom_client + " " + Client.Prenom_client
      					ListeAjoute(COMBO_client, np)
      				FIN
      			FIN
      		FIN
      	FIN
     type : 458752
   -
     name : InitialisationCOMBO_Actif
     procedure_id : 1566323209363212705
     type_code : 14
     code : |1-
      // Résumé : <indiquez ici ce que fait la procédure>
      // Syntaxe :
      // InitialisationCOMBO_Actif ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      // Exemple :
      // <Indiquez ici un exemple d'utilisation>
      //
      procédure InitialisationCOMBO_Actif(nomClient)
      	idClient est un entier sur 8 octet = TrouverIdClient(nomClient)
      	ListeSupprimeTout(COMBO_Actifs)
      	POUR TOUT Actifs 
      		SI Actifs.IDClient = idClient ALORS
      			ListeAjoute(COMBO_Actifs, Actifs.Nom_actif)
      		FIN
      	FIN
     type : 458752
   -
     name : TrouverIdClient
     procedure_id : 1566323565845974911
     type_code : 14
     code : |1-
      // Résumé : <indiquez ici ce que fait la procédure>
      // Syntaxe :
      // TrouverIdClient ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      // Exemple :
      // <Indiquez ici un exemple d'utilisation>
      //
      procédure TrouverIdClient(nomClient)
      
      	valeurComboClient est une chaine
      	valeurComboClient = nomClient
      	
      	// Extraction du Nom et Prénom à partir de la valeur de la combo
      	Nomm est une chaîne
      	Nomm = ExtraitChaîne(valeurComboClient, 1, " ")
      	Prenom est une chaîne
      	Prenom = ExtraitChaîne(valeurComboClient, 2, " ")
      	
      	//Recherche de l'id correspondant
      	ID_ClientSelectionne est un entier sur 8 octet
      	ID_ClientSelectionne = -1
      	
      	POUR TOUT Client
      		SI Client.Nom_client = Nomm ET Client.Prenom_client = Prenom ALORS
      			// Trouvé le client correspondant
      			ID_ClientSelectionne = Client.IDClient
      			//Info(ID_ClientSelectionne)
      			SORTIR // Sortir de la boucle puisque l'id a été trouvé
      		FIN
      	FIN
      	
      	RENVOYER ID_ClientSelectionne
     type : 458752
   -
     name : InitialisationCOMBO_estimation
     procedure_id : 1566324862928955316
     type_code : 14
     code : |1-
      // Résumé : <indiquez ici ce que fait la procédure>
      // Syntaxe :
      //InitialisationCOMBO_estimation (<nomClient>, <nomActif>)
      //
      // Paramètres :
      // 	nomClient : <indiquez ici le rôle de nomClient>
      // 	nomActif : <indiquez ici le rôle de nomActif>
      // Valeur de retour :
      // 	Aucune
      //
      // Exemple :
      // <Indiquez ici un exemple d'utilisation>
      //
      procédure InitialisationCOMBO_estimation(nomClient, nomActif)
      	
      	//Permet de réinitialiser à chaque changement de valeurs pour client et actif
      	ListeSupprimeTout(COMBO_Estimations_de_risques)
      	
      	idClient est un entier sur 8 octets = TrouverIdClient(nomClient)
      	idActif est un entier sur 8 octets = TrouveIdActif(nomActif)
      	textEstimationDefaut est une chaîne = ""
      	
      	//Vérif si il a sélectionner un client et actif sinon l'alerte
      	SI SAI_actif = "" ET SAI_client <> "" ALORS
      		ToastAffiche("Sélectionner un actif.")
      		retour
      	FIN
      	
      	SI SAI_actif <> "" ET SAI_client = "" ALORS
      		ToastAffiche("Sélectionner un client.")
      		retour 
      	FIN
      
      	//Si tout est rempli, initialise le combo
      	si SAI_actif <> "" et SAI_client <> "" alors
      		
      		POUR TOUT Evaluation_Risque
      			SI Evaluation_Risque.IDClient = idClient ET Evaluation_Risque.IDActifs = idActif ALORS
      				// L'idMenace correspondant a été trouvé
      				idMenaceTrouve est un entier sur 8 octets = Evaluation_Risque.IDMenace
      				pour TOUT Menace 
      					si idMenaceTrouve = Menace.IDMenace alors
      						ListeAjoute(COMBO_Estimations_de_risques,Menace.Nom_menace)
      					FIN
      				FIN
      			Fin 		
      		Fin
      	FIn 
     type : 458752
   -
     name : SupprimerEstimationRisque
     procedure_id : 1566342777260068403
     type_code : 14
     code : |1+
      // Résumé : <indiquez ici ce que fait la procédure>
      // Syntaxe :
      // SupprimerEstimationRisque ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      // Exemple :
      // <Indiquez ici un exemple d'utilisation>
      //
      procédure SupprimerEstimationRisque()
      
      	si SAI_estimation <> "" ALORS
      		pour TOUT Evaluation_Risque 
      			si Evaluation_Risque.IDEvaluation_risque=TrouveIdEstimation(SAI_estimation) ALORS
      //				info(Evaluation_Risque.IDEvaluation_risque)
      				//Demander confirmation avant de supprimer
      				SI OuiNon("Voulez-vous vraiment supprimer cette estimation (n°"+Evaluation_Risque.IDEvaluation_risque+") ?") = Oui ALORS
      					HSupprime(Evaluation_Risque)
      					ToastAffiche("L'estimation n°"+Evaluation_Risque.IDEvaluation_risque+" a été supprimée.")
      					//On supprime aussi dans Rapport_Evaluation s'il existe
      					pour TOUT Rapport_Evaluation 
      						si TrouveIdEstimation(SAI_estimation) = Rapport_Evaluation.IDEvaluation_risque ALORS
      							HSupprime(Rapport_Evaluation)
      						FIN
      					FIN
      					//RAZ
      					RemiseAZero()
      				SINON
      					ToastAffiche("Suppression annulée.")
      					RemiseAZero()
      				FIN
      			FIn
      		FIN
      		//Supprimer aussi 
      	sinon
      		ToastAffiche("Veuillez remplir tout les champs.")
      	FIN
     type : 458752
   -
     name : TrouveIdActif
     procedure_id : 1566344847436229320
     type_code : 14
     code : |1+
      // Résumé : <indiquez ici ce que fait la procédure>
      // Syntaxe :
      // TrouveIdActif ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      // Exemple :
      // <Indiquez ici un exemple d'utilisation>
      //
      PROCÉDURE TrouveIdActif(nomActif)
      idActif est un entier sur 8 octets
      
      POUR TOUT Actifs
      	SI nomActif=Actifs.Nom_actif ALORS
      		idActif=Actifs.IDActifs
      	FIN
      FIN
      
      RENVOYER idActif
      
     type : 458752
   -
     name : TrouveIdEstimation
     procedure_id : 1566345693546556230
     type_code : 14
     code : |1-
      // Résumé : <indiquez ici ce que fait la procédure>
      // Syntaxe :
      //[ <Résultat> = ] TrouveIdEstimation (<nomEstimation>)
      //
      // Paramètres :
      //	nomEstimation : <indiquez ici le rôle de nomEstimation>
      // Valeur de retour :
      // 	entier sur 8 octets : <indiquez ici le rôle de la valeur de retour>
      //
      // Exemple :
      // <Indiquez ici un exemple d'utilisation>
      //
      procédure TrouveIdEstimation(nomEstimation)
      
      idActif est un entier sur 8 octets = TrouveIdActif(SAI_actif)
      idClient est un entier sur 8 octets = TrouverIdClient(SAI_client)
      idMenace est un entier sur 8 octets
      idEstimation est un entier sur 8 octets
      
      pour TOUT Menace 
      	si Menace.Nom_menace = nomEstimation ALORS
      		idMenace = Menace.IDMenace
      	FIN
      FIN
      
      pour TOUT Evaluation_Risque 
      	si Evaluation_Risque.IDActifs = idActif et Evaluation_Risque.IDClient = idClient et Evaluation_Risque.IDMenace = idMenace ALORS
      		idEstimation = Evaluation_Risque.IDEvaluation_risque
      	FIN
      FIN
      
      renvoyer idEstimation
     type : 458752
   -
     name : RemiseAZero
     procedure_id : 1566353175390044228
     type_code : 14
     code : |1-
      // Résumé : <indiquez ici ce que fait la procédure>
      // Syntaxe :
      // RemiseAZero ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      // Exemple :
      // <Indiquez ici un exemple d'utilisation>
      //
      procédure RemiseAZero()
      	SAI_estimation = ""
      	SAI_actif = ""
      	SAI_client = ""
      	ListeSupprimeTout(COMBO_Actifs)
      	ListeSupprimeTout(COMBO_client)
      	ListeSupprimeTout(COMBO_Estimations_de_risques)
     type : 458752
